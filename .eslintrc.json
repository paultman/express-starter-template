{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    // Allow dangling underscores
    // http://eslint.org/docs/rules/no-underscore-dangle
    "no-underscore-dangle": [0]
  }
}
